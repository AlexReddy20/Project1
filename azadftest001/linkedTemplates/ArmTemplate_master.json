{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azadftest001"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Sourceblob001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sourceblob001'"
		},
		"Tragetgen2001_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Tragetgen2001'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdatalakegen2.dfs.core.windows.net/"
		},
		"HttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().Baseurl}"
		},
		"Tragetgen2001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdatalakegen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azadftest001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Sourceblob001_connectionString": {
						"value": "[parameters('Sourceblob001_connectionString')]"
					},
					"Tragetgen2001_accountKey": {
						"value": "[parameters('Tragetgen2001_accountKey')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"HttpServer_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_properties_typeProperties_url')]"
					},
					"Tragetgen2001_properties_typeProperties_url": {
						"value": "[parameters('Tragetgen2001_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}